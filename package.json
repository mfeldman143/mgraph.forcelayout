    "typescript": "^5.7.3",
    "vitest": "^3.1.4",
    "vue": "^3.5.16"
  },
  "dependencies": {
    "mgraph.events": "^1.0.0",
    "mgraph.merge": "^1.0.0",
    "mgraph.random": "^1.0.0"
